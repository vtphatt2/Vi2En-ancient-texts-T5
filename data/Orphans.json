{
    "file_name": "Orphans",
    "data": [
        {
            "id": 0,
            "vi": "Mồ Côi",
            "en": "Orphans"
        },
        {
            "id": 1,
            "vi": "Con chim non rũ cánh",
            "en": "A baby bird, with drooping wings,"
        },
        {
            "id": 2,
            "vi": "Đi tìm tổ bơ vơ",
            "en": "goes looking for its nest, forlorn,"
        },
        {
            "id": 3,
            "vi": "Quanh nẻo rừng hiu quạnh",
            "en": "around the silent, lonesome woods,"
        },
        {
            "id": 4,
            "vi": "Lướt mướt dưới dòng mưa.",
            "en": "all wet and dripping with the rain."
        },
        {
            "id": 5,
            "vi": "Con chim non chiu chít",
            "en": "The birdie sadly cheeps and cheeps;"
        },
        {
            "id": 6,
            "vi": "Lá động khóc tràn trề",
            "en": "and moved, the leaves shed tears on tears."
        },
        {
            "id": 7,
            "vi": "Chao ôi buồn da diết",
            "en": "O Heaven, it’s so sad it hurts."
        },
        {
            "id": 8,
            "vi": "Chim ơi biết đâu về.",
            "en": "O birdie, where will you go home?"
        },
        {
            "id": 9,
            "vi": "Gió lùa mưa rơi rơi",
            "en": "The wind whips past—rain drops and drops"
        },
        {
            "id": 10,
            "vi": "Trên nẻo đường sương lạnh",
            "en": "upon the chilly forest trail."
        },
        {
            "id": 11,
            "vi": "Đi về đâu em ơi",
            "en": "Where will you, little bird, go home"
        },
        {
            "id": 12,
            "vi": "Phơi thân tàn cô quạnh!",
            "en": "and dry your withered body out?"
        },
        {
            "id": 13,
            "vi": "Em sưởi trong bàn tay",
            "en": "Come to my hands and warm yourself"
        },
        {
            "id": 14,
            "vi": "Cho lòng băng giá ấm",
            "en": "so that your frozen heart will thaw."
        },
        {
            "id": 15,
            "vi": "Lìa cành lá bay bay",
            "en": "Get out of where the wind goes mad"
        },
        {
            "id": 16,
            "vi": "Như mảnh đời u thảm!",
            "en": "beneath a sky all dark with clouds."
        },
        {
            "id": 17,
            "vi": "Con chim non không tổ",
            "en": "The baby bird without a nest,"
        },
        {
            "id": 18,
            "vi": "Trẻ mồ côi không nhà",
            "en": "the orphan child without a home:"
        },
        {
            "id": 19,
            "vi": "Hai đứa cùng đau khổ",
            "en": "both wretches suffer pain and grief,"
        },
        {
            "id": 20,
            "vi": "Cùng vất vưởng bê tha",
            "en": "both strays and waifs roam here and there."
        },
        {
            "id": 21,
            "vi": "Rồi ngày kia rã cánh",
            "en": "Some day, their wings will fall in shreds—"
        },
        {
            "id": 22,
            "vi": "Rụi chết bên đường đi...",
            "en": "they’ll slump beside the road and die."
        },
        {
            "id": 23,
            "vi": "Thờ ơ con mắt lạnh",
            "en": "All passers-by, with cold, blank eyes,"
        },
        {
            "id": 24,
            "vi": "Nhìn chúng: “Có hề chi!”",
            "en": "will cast a glance and say, “So what?”"
        }
    ]
}